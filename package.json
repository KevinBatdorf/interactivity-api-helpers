{
	"name": "interactivity-api-helpers",
	"version": "0.1.0",
	"description": "Helper library for building with the WordPress Interactivity API",
	"author": "Kevin Batdorf",
	"homepage": "https://github.com/KevinBatdorf/interactivity-api-helpers",
	"repository": "github:KevinBatdorf/interactivity-api-helpers",
	"license": "MIT",
	"keywords": [
		"interactivity api",
		"wordpress",
		"vite",
		"typescript",
		"npm",
		"package"
	],
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite build --watch",
		"start": "vite --host",
		"build": "vite build",
		"build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
		"eslint": "eslint ./src --ext .ts",
		"test": "vitest",
		"e2e": "npm run build && mkdir -p ./block/build && cp -R ./dist/* ./block/build"
	},
	"peerDependencies": {
		"@wordpress/interactivity": "^5.6.0"
	},
	"devDependencies": {
		"@babel/runtime": "^7.24.5",
		"@playwright/test": "^1.43.1",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^20.12.8",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"dts-bundle-generator": "^9.5.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"happy-dom": "^14.7.1",
		"prettier": "^3.2.5",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-dts": "^3.9.0",
		"vitest": "^1.6.0"
	}
}
